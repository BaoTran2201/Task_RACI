{
  "rewrites": [
    {
      "source": "/public/data/(.*)",
      "destination": "/public/data/$1"
    }
  ],
  "headers": [
    {
      "source": "/public/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}